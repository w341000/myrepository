<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
<!-- 产品类别 -->
	<package name="uploadfile" namespace="/control/uploadfile" extends="control-default">
		<action name="list" class="uploadFileAction"><result >/WEB-INF/page/uploadfile/uploadfilelist.jsp</result></action>
		<action name="manage_*" class="uploadFileManageAction" method="{1}">
			<result name="uploadUI">/WEB-INF/page/uploadfile/upload.jsp</result>
			<result name="uploadfinish">/WEB-INF/page/uploadfile/fileuploadfinish.jsp</result>
			<result name="error">/WEB-INF/page/uploadfile/error.jsp</result>
		</action>
	</package>
</struts>    
