<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	
	<!-- 权限组管理处理 -->
	<package name="privilegegroup" namespace="/control/privilegegroup" extends="control-default">
		
		
		<action name="list" class="privilegeGroupListAction" >
			<result>/WEB-INF/page/department/privilegegrouplist.jsp</result>
		</action>
		<action name="manage_*" class="privilegeGroupManageAction" method="{1}" >
			<result name="add">/WEB-INF/page/department/addprivilegegroup.jsp</result>
			<result name="edit">/WEB-INF/page/department/editprivilegegroup.jsp</result>
		</action>
		
	</package>
	

</struts>    
