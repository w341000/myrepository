<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<!-- 产品管理-->
		<package name="productstyle"  namespace="/control/product/style"  extends="control-default" >
		<action name="list" class="productStyleAction"><result >/WEB-INF/page/product/productstylelist.jsp</result></action>
		<action name="manage_*" class="productStyleManageAction" method="{1}">
			<result name="addUI">/WEB-INF/page/product/add_productstyle.jsp</result>
			<result name="editUI">/WEB-INF/page/product/edit_productstyle.jsp</result>
		</action>
	</package>
</struts>    
