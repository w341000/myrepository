<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<!-- 品牌类别 -->
		<package name="brand" namespace="/control/brand" extends="control-default">
		<action name="list" class="brandAction"><result >/WEB-INF/page/product/brandlist.jsp</result></action>
		<action name="manage_*" class="brandManageAction" method="{1}">
			<result name="addUI">/WEB-INF/page/product/add_brand.jsp</result>
			<result name="editUI">/WEB-INF/page/product/edit_brand.jsp</result>
			<result name="queryUI">/WEB-INF/page/product/query_brand.jsp</result>
		</action>
	</package>
</struts>    
