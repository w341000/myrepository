<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
<!-- 产品类别 -->
	<package name="producttype" namespace="/control/product/type" extends="control-default">
		
		<action name="list" class="productTypeAction"><result >/WEB-INF/page/product/producttypelist.jsp</result></action>
		<action name="manage_*" class="productTypeManageAction" method="{1}">
			<result name="addUI">/WEB-INF/page/product/add_productType.jsp</result>
			<result name="editUI">/WEB-INF/page/product/edit_productType.jsp</result>
			<result name="queryUI">/WEB-INF/page/product/query_productType.jsp</result>
		</action>
	</package>
</struts>    
